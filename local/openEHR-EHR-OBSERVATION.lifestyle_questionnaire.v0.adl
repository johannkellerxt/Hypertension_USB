definition
	OBSERVATION[at0000] matches {    -- Lifestyle questionnaire
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0004] occurrences matches {0..1} matches {    -- Screening purposes
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0005] occurrences matches {0..*} matches {*}    -- Any symptoms or signs?
									ELEMENT[at0006] occurrences matches {0..*} matches {    -- Onset
										value matches {
											DV_DATE_TIME matches {*}
											DV_DURATION matches {*}
											DV_INTERVAL<DV_DATE_TIME> matches {*}
											DV_INTERVAL<DV_DATE> matches {*}
											DV_CODED_TEXT matches {*}
											DV_DATE matches {*}
										}
									}
									ELEMENT[at0007] occurrences matches {0..1} matches {    -- Description
										value matches {
											DV_TEXT matches {*}
										}
									}
									CLUSTER[at0008] occurrences matches {0..*} matches {    -- Lifestyle category
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0009] occurrences matches {0..1} matches {    -- Category name
												value matches {
													DV_TEXT matches {*}
												}
											}
											ELEMENT[at0010] occurrences matches {0..1} matches {    -- Duration
												value matches {
													C_DV_QUANTITY <
														property = <[openehr::128]>
														list = <
															["1"] = <
																units = <"d">
															>
															["2"] = <
																units = <"h">
															>
														>
													>
												}
											}
											ELEMENT[at0011] occurrences matches {0..1} matches {    -- Frequency
												value matches {
													C_DV_QUANTITY <
														property = <[openehr::128]>
														list = <
															["1"] = <
																units = <"h">
															>
															["2"] = <
																units = <"d">
															>
														>
													>
												}
											}
											ELEMENT[at0012] occurrences matches {0..*} matches {    -- Type
												value matches {
													DV_TEXT matches {*}
													DV_CODED_TEXT matches {*}
													DV_DATE matches {*}
													DV_DURATION matches {*}
												}
											}
											CLUSTER[at0017] occurrences matches {0..*} matches {    -- Details about this item
												items cardinality matches {1..*; unordered} matches {
													ELEMENT[at0018] occurrences matches {0..1} matches {    -- Item name
														value matches {
															DV_TEXT matches {*}
														}
													}
													ELEMENT[at0019] occurrences matches {0..1} matches {    -- Frequency
														value matches {
															DV_QUANTITY matches {*}
															DV_TIME matches {*}
														}
													}
													ELEMENT[at0021] occurrences matches {0..1} matches {    -- Type
														value matches {
															DV_TEXT matches {*}
															DV_CODED_TEXT matches {*}
															DV_DATE_TIME matches {*}
															DV_DURATION matches {*}
														}
													}
													ELEMENT[at0020] occurrences matches {0..1} matches {    -- Comment
														value matches {
															DV_TEXT matches {*}
														}
													}
												}
											}
											ELEMENT[at0013] occurrences matches {0..1} matches {    -- Comment
												value matches {
													DV_TEXT matches {*}
												}
											}
											allow_archetype CLUSTER[at0014] occurrences matches {0..*} matches {    -- Additional details
												include
													archetype_id/value matches {/.*/}
											}
										}
									}
									allow_archetype CLUSTER[at0016] occurrences matches {0..*} matches {    -- Additional details
										include
											archetype_id/value matches {/.*/}
									}
								}
							}
						}
					}
				}
			}
		}
	}

